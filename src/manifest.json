{
  "name": "mealshot",
  "description": "Let us pick your meal!",
  "version": 1.0,
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "192": "icons/icon_192.png",
    "512": "icons/icon_512.png"
  },
  "browser_action": {
    "default_title": "mealshot",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "https://mealshot.test/",
    "geolocation"
  ]
}
